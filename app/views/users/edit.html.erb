<h1> Edit User </h1>
<%= form_for @user do |f| %>

  <%= f.label :firstName %>:
  <%= f.text_field :firstName %>

  <%= f.label :lastName %>
  <%= f.text_field :lastName %>

  <%= f.label :age %>
  <%= f.integer_field :age %>

  <%= f.label :zipcode %>
  <%= f.integer_field :zipcode %>

  <%= f.label :phone %>
  <%= f.integer_field :phone %>

  <%= f.label :long %>
  <%= f.integer_field :long %>

  <%= f.label :lat %>
  <%= f.integer_field :lat %>

  
<%= f.submit %>
<% end %>