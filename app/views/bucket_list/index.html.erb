<h1>BucketList#index</h1>
<p>Find me in app/views/bucket_list/index.html.erb</p>
<h1> Bucketlist </h1>
<% @bucket_lists.each do |bucketlist| %>
  <h2><%= bucketlist.title %></h2>
  <h3><%= bucketlist.user_id %></h3>

<p><%= link_to 'Show bucketlist', bucket_list_path(bucketlist) %></p>
<p><%= link_to 'Edit bucketlist', edit_bucket_list_path(bucketlist) %></p>
<p><%= link_to 'New bucketlist', new_bucket_list_path %></p>
<% end %>

